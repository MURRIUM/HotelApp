<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="api.Entity.ServicesEntity" table="servicesTable" schema="hotel">
        <id name="idService">
            <column name="id_service" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="Name" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="price">
            <column name="Price" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>