<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="api.Entity.CharacteristicsEntity" table="characteristics" schema="hotel">
        <id name="idCharacteristic">
            <column name="id_characteristic" sql-type="int(11)"/>
        </id>
        <property name="nameCharacteristic">
            <column name="Name_characteristic" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="description">
            <column name="Description" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>